{
  "name": "redirection",
  "version": "2.8 beta 1",
  "description": "Redirection is a WordPress plugin to manage 301 redirections and keep track of 404 errors without requiring knowledge of Apache .htaccess files.",
  "main": "redirection.php",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "testsetup": "bin/install-wp-tests.sh wordpress_test wp wp localhost latest",
    "start": "NODE_ENV=development DEBUG=tinker* webpack-dev-server --progress --colors --hot --config ./webpack.config.js --port 3312",
    "dist": "NODE_ENV=production webpack --progress --colors --config ./webpack.config.js",
    "localprod": "NODE_ENV=production webpack-dev-server --progress --colors --hot --config ./webpack.config.js --port 3312",
    "lint": "eslint --cache --ext=.js src",
    "stat": "NODE_ENV=production webpack --json | webpack-bundle-size-analyzer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngodley/redirection.git"
  },
  "keywords": [
    "post",
    "admin",
    "seo",
    "pages",
    "manage",
    "301",
    "404",
    "redirect",
    "permalink"
  ],
  "author": "John Godley",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/johngodley/redirection/issues"
  },
  "homepage": "https://github.com/johngodley/redirection",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-wpcalypso": "^1.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-wpcalypso": "^4.0.0",
    "exports-loader": "^0.6.4",
    "glob-all": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-deleted": "0.0.2",
    "gulp-po2json": "^0.4.0",
    "gulp-sort": "^2.0.0",
    "gulp-wp-pot": "^2.0.6",
    "gulp-zip": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "i18n-calypso": "^1.8.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "path": "^0.12.7",
    "postcss-cssnext": "^3.0.2",
    "postcss-focus": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^5.0.0",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "shared-git-hooks": "^1.2.1",
    "style-loader": "^0.19.0",
    "through": "^2.3.8",
    "underscore": "^1.8.3",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-remove-debug": "^0.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "extract-text-webpack-plugin": "3.0.1",
    "interpolate-components": "^1.1.0",
    "jed": "^1.1.1",
    "lru": "^3.1.0",
    "plite": "^0.0.5",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.1.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true
    },
    "extends": [
      "wpcalypso/react",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "rules": {
      "max-len": 0,
      "wpcalypso/jsx-classname-namespace": 0,
      "wpcalypso/import-no-redux-combine-reducers": 0
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "jest": {
    "modulePaths": [
      "./client"
    ]
  }
}
