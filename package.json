{
	"name": "redirection",
	"version": "5.4.0-beta-1",
	"description": "Redirection is a WordPress plugin to manage 301 redirections and keep track of 404 errors without requiring knowledge of Apache .htaccess files.",
	"engines": {
		"node": ">=12.14.0",
		"npm": ">=6.14.8"
	},
	"main": "redirection.php",
	"browser": {
		"redirection.php": false
	},
	"sideEffects": true,
	"dependencies": {
		"@emotion/react": "^11.10.4",
		"@wordpress/element": "^4.14.0",
		"@wordpress/i18n": "^4.16.0",
		"classnames": "^2.3.1",
		"deep-equal": "^2.0.5",
		"qs": "^6.11.0",
		"rc-progress": "^3.4.0",
		"rc-util": "^5.24.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dropzone": "^14.2.2",
		"react-focus-lock": "^2.9.1",
		"react-highlight-words": "^0.18.0",
		"react-redux": "^8.0.2",
		"react-textarea-autosize": "^8.3.4",
		"redux": "^4.2.0",
		"redux-thunk": "^2.4.1",
		"typescript": "^4.8.2",
		"url": "^0.11.0",
		"use-debounce": "^8.0.4"
	},
	"devDependencies": {
		"@redux-devtools/extension": "^3.2.3",
		"@types/jest": "^29.0.0",
		"@types/react": "^18.0.18",
		"@types/react-dom": "^18.0.6",
		"@types/react-highlight-words": "^0.16.4",
		"@types/react-redux": "^7.1.24",
		"@wordpress/base-styles": "^4.7.0",
		"@wordpress/eslint-plugin": "^13.0.0",
		"@wordpress/prettier-config": "^2.0.0",
		"@wordpress/scripts": "^24.0.0",
		"apidoc": "^0.53.0",
		"chai": "^4.3.6",
		"download": "^8.0.0",
		"glob-all": "^3.3.0",
		"gulp": "^4.0.2",
		"gulp-deleted": "1.0.0",
		"gulp-po2json": "^1.0.0",
		"gulp-zip": "^5.1.0",
		"he": "^1.2.0",
		"jest": "^29.0.2",
		"jest-environment-jsdom": "^29.0.2",
		"mocha": "^10.0.0",
		"node-fetch": "2",
		"path": "^0.12.7",
		"prettier": "npm:wp-prettier@2.6.2",
		"release-it": "^15.4.1",
		"request": "^2.88.2",
		"through": "^2.3.8",
		"through2": "^4.0.2",
		"webpack-shell-plugin-next": "^2.2.2"
	},
	"scripts": {
		"build": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:php": "phpcs -s",
		"lint:psalm": "./vendor/vimeo/psalm/psalm --show-info=true",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:phan": "./vendor/bin/phan",
		"lint:stan": "./vendor/bin/phpstan analyse -c phpstan.neon",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip && mv redirection.zip redirection-$npm_package_version.zip",
		"release": "release-it",
		"start": "wp-scripts start",
		"test:e2e": "NODE_ENV=e2e jest --config=e2e-jest.js --testPathIgnorePatterns='/node_modules/|/client/' --testTimeout=30000 e2e",
		"test:js": "wp-scripts test-unit-js",
		"test:php": "composer run-script test",
		"test:unit": "wp-scripts test-unit-js",
		"locale": "rm -rf language/*.po language/*.mo language/json/*.json && composer run-script pot && yarn locale:download && yarn locale:json",
		"locale:download": "gulp potDownload",
		"locale:json": "gulp potJson",
		"doc": "apidoc -v -i api/ -o ./api-doc/",
		"svn": "gulp svn",
		"plugin": "gulp plugin"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johngodley/redirection.git"
	},
	"keywords": [
		"post",
		"admin",
		"seo",
		"pages",
		"manage",
		"301",
		"404",
		"redirect",
		"permalink"
	],
	"author": "John Godley",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/johngodley/redirection/issues"
	},
	"homepage": "https://github.com/johngodley/redirection",
	"apidoc": {
		"name": "Redirection REST API",
		"version": "0.1.0",
		"description": "WordPress REST API documentation for Redirection",
		"title": "Redirection REST API",
		"url": "https://redirection.me/wp-json",
		"header": {
			"title": "Introduction",
			"filename": "./api-header.md"
		},
		"footer": {
			"filename": "./api-footer.md"
		},
		"template": {
			"withCompare": false
		}
	},
	"wordpress": {
		"supported": "5.4"
	}
}
